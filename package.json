{
  "name": "@big-whale-labs/farcantasy-arena-contract",
  "version": "0.0.1",
  "description": "Farcantasy arena contract",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BigWhaleLabs/farcantasy-arena-contract"
  },
  "license": "MIT",
  "np": {
    "2fa": false,
    "tests": false,
    "cleanup": false,
    "yarn": false
  },
  "files": [
    "dist/typechain/**/*",
    "contracts/**/*.sol"
  ],
  "main": "dist/typechain/index.js",
  "types": "dist/typechain/index.d.ts",
  "scripts": {
    "build": "yarn clean-cache && yarn hardhat compile",
    "clean-cache": "yarn hardhat clean",
    "test": "NODE_ENV=test yarn hardhat test",
    "coverage": "yarn hardhat compile --force && node --max_old_space_size=6144 node_modules/.bin/hardhat coverage --testfiles 'test/**/*.ts' --show-stack-traces",
    "deploy": "yarn hardhat run --network deploy scripts/deploy.ts",
    "eth-lint": "yarn solhint 'contracts/**/*.sol'",
    "lint": "prettier --check . && eslint --max-warnings 0 --ext ts scripts test && yarn eth-lint",
    "prettify": "prettier --write .",
    "release": "np",
    "prepublishOnly": "yarn build && yarn tsc",
    "generate-inputs": "ts-node scripts/generate-inputs.ts",
    "compile": "yarn compile-selection && yarn compile-reveal",
    "compile-selection": "scripts/compile-circuit.sh CardSelection card-selection 12",
    "compile-reveal": "scripts/compile-circuit.sh CardReveal card-reveal 9",
    "download-ptau": "yarn download-ptau-9 && yarn download-ptau-12",
    "download-ptau-9": "curl https://hermez.s3-eu-west-1.amazonaws.com/powersOfTau28_hez_final_09.ptau -o pot/pot9_final.ptau",
    "download-ptau-12": "curl https://hermez.s3-eu-west-1.amazonaws.com/powersOfTau28_hez_final_12.ptau -o pot/pot12_final.ptau"
  },
  "devDependencies": {
    "@big-whale-labs/versioned-contract": "^1.0.2",
    "@ethersproject/providers": "^5.7.2",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.5",
    "@openzeppelin/contracts": "^4.9.2",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "chai": "^4.3.7",
    "circom_tester": "^0.0.19",
    "circomlib": "^2.0.5",
    "circomlibjs": "^0.1.7",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "ethereum-waffle": "^4.0.10",
    "ethers": "^5.7.2",
    "hardhat": "^2.13.0",
    "hardhat-gas-reporter": "^1.0.9",
    "np": "^8.0.4",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "prompt": "^1.3.0",
    "snarkjs": "^0.7.0",
    "solhint": "^3.4.1",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^5.1.3"
  },
  "packageManager": "yarn@3.6.0"
}
